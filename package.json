{
  "name": "express-starter",
  "version": "1.0.0",
  "engines": {
    "node": "6.9.4",
    "npm": "3.10.10"
  },
  "main": "index.js",
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "bunyan": "^1.8.10",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "helmet": "^3.5.0",
    "morgan": "^1.8.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.13.0",
    "eslint-config-ericmasiello": "^1.2.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.45.0",
    "flow-typed": "^2.1.2",
    "jest": "^20.0.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.4.1",
    "webpack-merge": "^4.1.0"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node ./build/server.js",
    "dev": "babel-node src/www/index.js | bunyan",
    "validate": "npm-run-all --parallel test flow:check lint",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "lint": "eslint . '**/.*.js' --cache",
    "lint:fix": "npm run lint -- --fix",
    "flow": "flow",
    "flow:check": "flow check",
    "flow:update": "flow-typed install",
    "flow:stop": "flow stop",
    "build": "npm-run-all --parallel build:server build:client",
    "prebuild:server": "rimraf build",
    "build:server": "webpack --progress --config ./tools/webpack/webpack.server.babel.js",
    "prebuild:client": "rimraf public/build",
    "build:client": "webpack --progress --config ./tools/webpack/webpack.client.babel.js --env=dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericmasiello/express-starter.git"
  },
  "keywords": [],
  "author": "Eric Masiello <eric.j.masiello@gmail.com> (http://synbydesign.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericmasiello/express-starter/issues"
  },
  "homepage": "https://github.com/ericmasiello/express-starter#readme",
  "description": "",
  "jest": {
    "collectCoverageFrom": [
      "src/server/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
