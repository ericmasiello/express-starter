{
  "name": "express-starter",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.1.4",
    "npm": ">=5.3.0"
  },
  "main": "index.js",
  "dependencies": {
    "bunyan": "^1.8.10",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "helmet": "^3.5.0",
    "morgan": "^1.8.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-require-ignore": "~0.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "css-modules-require-hook": "^4.0.6",
    "empty": "^0.10.1",
    "enzyme": "^2.9.1",
    "eslint": "^3.13.0",
    "eslint-config-ericmasiello": "^1.2.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.46.0",
    "flow-typed": "^2.1.2",
    "html-webpack-plugin": "~2.29.0",
    "husky": "^0.14.0",
    "jest": "^20.0.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "raw-loader": "~0.5.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-ericmasiello": "^1.0.0",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node ./build/www",
    "dev:copyImages": "rimraf public/images && mkdir public/images && cp -r src/client/images/* public/images/",
    "dev:client": "cross-env webpack --progress --env=dev",
    "predev": "npm-run-all dev:copyImages dev:client",
    "dev": "nodemon --ignore src/client src/www/dev.js | bunyan",
    "validate": "npm-run-all --parallel test flow lint lint:style",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "lint": "eslint . '**/.*.js' --cache",
    "lint:fix": "npm run lint -- --fix",
    "lint:style": "stylelint 'src/**/*.scss'",
    "flow": "flow check",
    "flow:update": "flow-typed install",
    "build": "npm-run-all --parallel build:server build:client",
    "prebuild:server": "rimraf build",
    "build:server": "cross-env BABEL_ENV=node babel src -d build --copy-files --source-maps --ignore test.js",
    "prebuild:client": "rimraf public/build && rimraf public/images",
    "build:client": "cross-env NODE_ENV=production webpack -p --progress --env=dist",
    "prepush": "npm run validate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericmasiello/express-starter.git"
  },
  "keywords": [],
  "author": "Eric Masiello <eric.j.masiello@gmail.com> (http://synbydesign.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericmasiello/express-starter/issues"
  },
  "homepage": "https://github.com/ericmasiello/express-starter#readme",
  "description": "",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/www/",
      "/src/config/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
